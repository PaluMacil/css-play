{
    "compilerOptions": {
        "sourceMap": true,
        "allowJs": true,
        "outFile": "./www/bundle.js",
        "noImplicitAny": true,
        "noImplicitThis": true,
        "noImplicitReturns": true,
        "strictFunctionTypes": true,
        "strictPropertyInitialization":true,
        "strictNullChecks": true,
        "noUnusedParameters": true,
        "module": "system",
        "target": "es5",
    },
    "include": [
        "app/**/*"
    ]
}